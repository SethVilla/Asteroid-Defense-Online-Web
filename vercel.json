{
  "routes": [
    {
      "src": "/Build/(.*)",
      "dest": "/Build/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ],
  "headers": [
    {
      "source": "/Build/(.*\\.data)",
      "headers": [
        { "key": "Content-Type", "value": "application/octet-stream" },
        { "key": "Content-Encoding", "value": "identity" }
      ]
    },
    {
      "source": "/Build/(.*\\.wasm)",
      "headers": [
        { "key": "Content-Type", "value": "application/wasm" }
      ]
    },
    {
      "source": "/Build/(.*\\.js)",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript" }
      ]
    },
    {
      "source": "/Build/(.*\\.framework\\.js\\.gz)",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript" },
        { "key": "Content-Encoding", "value": "gzip" }
      ]
    },
    {
      "source": "/Build/(.*\\.data\\.gz)",
      "headers": [
        { "key": "Content-Type", "value": "application/gzip" },
        { "key": "Content-Encoding", "value": "gzip" }
      ]
    },
    {
      "source": "/Build/(.*\\.wasm\\.gz)",
      "headers": [
        { "key": "Content-Type", "value": "application/wasm" },
        { "key": "Content-Encoding", "value": "gzip" }
      ]
    },
    {
      "source": "/Build/(.*\\.framework\\.js\\.br)",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript" },
        { "key": "Content-Encoding", "value": "br" }
      ]
    },
    {
      "source": "/Build/(.*\\.data\\.br)",
      "headers": [
        { "key": "Content-Type", "value": "application/octet-stream" },
        { "key": "Content-Encoding", "value": "br" }
      ]
    },
    {
      "source": "/Build/(.*\\.wasm\\.br)",
      "headers": [
        { "key": "Content-Type", "value": "application/wasm" },
        { "key": "Content-Encoding", "value": "br" }
      ]
    }
  ]
}